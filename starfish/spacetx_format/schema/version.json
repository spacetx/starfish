{
  "$id": "https://github.com/spacetx/starfish/spacetx_format/schema/version.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "string",
  "description": "The version number of the schema in major.minor.patch format. (Unversioned schema)",
  "pattern": "^[0-9]{1,}.[0-9]{1,}.[0-9]{1,}$",
  "example": "1.0.0"
}
